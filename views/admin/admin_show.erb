<h1>Details for gig #<%= @gig.id %></h1>
<ul>
  <p>Date: <%= @gig.date.strftime('%m/%d/%Y') %></p>
  <p>Band: <%= @gig.band_name %></p>
  <p>Music link: <%= @gig.music_link %></p>
</ul>

<form method="POST" action="/admin/gigs/<%= @gig.id %>/destroy">
  <label for= "submit">Delete Gig# <%= @gig.id %></label>
  <input type="submit" name= "submit"/>
</form>